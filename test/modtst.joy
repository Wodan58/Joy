			un == one;
			deux == zero one un + +
			IN
	two == deux;
	three == zero un + deux +;
			END;
	four == two three + one -;
			HIDE
			trois == two succ;
					HIDE
					eins == one;
					zwei == one dup +;
					drei == eins zwei +;
					IN
			quatre == drei eins +;
					END;
			cinq == two trois +
			IN
	five == cinq;
	six == trois three +;
	seven == trois drei + succ;
			END;
END

